		<!-- Declaracion de objetos que estarán en el ambiente -->
		<env:objecttype name="automovil">
					<env:property name="vision_range">2</env:property>
					<env:property name="position" event="true"/>
					<env:property name="velocidad" event="true"/>
		</env:objecttype>
		
		<env:objecttype name="semaforo">
					<env:property name="vision_range">4</env:property>
					<env:property name="position" event="true"/>
					<env:property name="lineaActual" class="int">0</env:property>
		</env:objecttype>
		<env:objecttype name="calleHE">
					<env:property name="velocidad">10</env:property>
					<env:property name="position"/>
		</env:objecttype>
		<env:objecttype name="calleHO">
					<env:property name="velocidad">10</env:property>
					<env:property name="position"/>
		</env:objecttype>
		<env:objecttype name="calleVN">
					<env:property name="velocidad">10</env:property>
					<env:property name="position"/>
		</env:objecttype>
		<env:objecttype name="calleVS">
					<env:property name="velocidad">10</env:property>
					<env:property name="position"/>
		</env:objecttype>
		<env:objecttype name="objetivo">
					<env:property name="position"/>
		</env:objecttype>
		<env:objecttype name="cuadra">
		<!-- Sin propiedades, es para crear los obstáculos -->
		</env:objecttype>
		
		<env:tasktypes>
				<env:tasktype name="move" class="MoveTask" />		
		</env:tasktypes>
		
	    <env:processtypes>
			<env:processtype name="crearObjetivo" class="DefaultObjectCreationProcess">
				<env:property name="timerate">5000</env:property>
				<env:property name="type">"objetivo"</env:property>
				<env:property name="properties" dynamic="true">
						posDisponible.obtenerPosicion()   
				</env:property>
			</env:processtype>
		</env:processtypes> 
		
		
		<env:dataviews>
				<env:dataview name="view_all" class="GeneralDataView2D" />
				<env:dataview name="semaforo_view" class="LocalDataView2D" objecttype="semaforo">
					<env:property name="object">$object</env:property>
					<env:property name="range">$object.vision_range</env:property>
				</env:dataview>
